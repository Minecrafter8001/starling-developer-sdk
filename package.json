{
  "name": "starling-developer-sdk",
  "version": "0.1.17",
  "description": "Starling Bank Developer SDK",
  "main": "dist/starling.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "gulp build",
    "test": "jest test",
    "test:dev": "jest test --watch",
    "test:verbose": "DEBUG=starling* npm test",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "jsdoc": "node_modules/.bin/jsdoc -c .jsdoc.json --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/starlingbank/starling-developer-js-sdk.git"
  },
  "keywords": [
    "starlingbank, starling, bank, open, API, SDK"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-modules-umd",
        {
          "globals": {
            "es6-promise": "Promise"
          }
        }
      ]
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "files": [
    "dist/*"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "debug": "^3.1.0",
    "es6-promise": "^4.2.4",
    "js-base64": "^2.1.9",
    "utf8": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-modules-umd": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "babelify": "^9.0.0",
    "browserify": "^16.2.3",
    "codecov": "^3.0.2",
    "eslint": "^4.19.1",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0-beta.2",
    "gulp-eslint": "^4.0.2",
    "gulp-jscs": "^4.1.0",
    "gulp-jscs-stylish": "^1.4.0",
    "gulp-rename": "^1.2.3",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.0",
    "jest": "^24.9.0",
    "jsdoc": "^3.5.5",
    "minami": "^1.2.3",
    "nock": "^9.3.2",
    "rimraf": "^3.0.0",
    "standard": "^14.3.1",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/starlingbank/starling-developer-js-sdk/issues"
  },
  "homepage": "https://github.com/starlingbank/starling-developer-js-sdk#readme"
}
